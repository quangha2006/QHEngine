{
	.PROJECT_Name = "zlibstatic"
	#include "../__Common/__STD_C.bff"

	.PROJECT_SourceFiles =
	{
		"$ExternalLibs_Path$/AssimpLib/contrib/zlib/adler32.c"
		"$ExternalLibs_Path$/AssimpLib/contrib/zlib/compress.c"
		"$ExternalLibs_Path$/AssimpLib/contrib/zlib/crc32.c"
		"$ExternalLibs_Path$/AssimpLib/contrib/zlib/deflate.c"
		"$ExternalLibs_Path$/AssimpLib/contrib/zlib/gzclose.c"
		"$ExternalLibs_Path$/AssimpLib/contrib/zlib/gzlib.c"
